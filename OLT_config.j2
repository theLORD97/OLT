bridge add 1-a-2-0/eth tls vlan 101 tagged
interface add 1-a-6-0/ipobridge vlan 101 {{ ip_addr1 }}/30
route add default {{ ip_addr2 }} 1
port up 1-a-2-0/eth
port description add 1-a-2-0/eth "Connected to {{ upstream_fqdn }} {{ upstream_port }}"


####### Follow Upgrade Software Procedure #########

# creating backup file
dump file {{ fqdn }}-{{ date }}.cfg


## Make sure file is created in folder ##
dir


## Create a new folder to back up current software image files ##
mkdir backup_SW
copy mxup2tg8gtopraw.bin backup_SW/mxup2tg8gtopraw.bin
copy mxup2tg8gtop.bin backup_SW/mxup2tg8gtop.bin      
copy mxlc20ae1scsfp.bin backup_SW/mxlc20ae1scsfp.bin

## Make sure files are copied to backup folder ##
dir backup_SW/

## Delete the current image files from /card1 ##
del mxup2tg8gtopraw.bin
del mxup2tg8gtop.bin   
del mxlc20ae1scsfp.bin

## Make sure files are deleted from directory ## 
dir

############################################################################
## download the new software (mxk2.5.1.581) image files from TFTP server ###
############################################################################

image download x.x.x.x mxup2tg8gtop.bin
image download x.x.x.x mxup2tg8gtopraw.bin
image download x.x.x.x mxlc20ae1scsfp.bin

image verify mxlc20ae1scsfp.bin
image verify mxup2tg8gtop.bin  
image verify mxup2tg8gtopraw.bin

## Initialize the flash card’s boot partition with the new image on the uplink card ##
image flash mxup2tg8gtopraw.bin 1

## Reboot system ##
systemreboot
